<h1>Livros disponíveis</h1>
<div class="books-container">
    <a href="/">Adicionar Livro</a>
    {{#each books}}
        <div>
            #{{this.id}} - <span class="bold">{{this.title}}</span> - {{this.author}} - {{this.pages}} Páginas - Lançamento {{this.launch_date}}
        </div>
    {{/each}}
</div>